#include <gtk/gtk.h>

int
main(int  argc,
char *argv[])
{
  ${1:gtk application$(str-to-style "upcamel" yas-text)} *app;
  int status;

  app = ${1:$(str-to-style "snake" yas-text)}_new ("${2:org.gnome.example}", G_APPLICATION_FLAGS_${3:NONE});
  status = g_application_run (G_APPLICATION (app), argc, argv);
  g_object_unref (app);

  return status;
}
