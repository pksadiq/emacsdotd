# -*- mode: snippet; require-final-newline: nil -*-
# name: <object> GtkButton
# key: ob.bu
# --
<object class="GtkButton" id="${1:button$(replace-regexp-in-string " " "_" yas-text)}">
<property name="label" translatable="yes">${2:_Button$(replace-regexp-in-string "^ " "_" yas-text)}</property>
<property name="use-underline">True</property>
<property name="visible">${3:True}</property>
<property name="can-focus">${4:True}</property>${0:$$(read-only-mode)}
</object>